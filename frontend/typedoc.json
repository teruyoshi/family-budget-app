{
  "entryPoints": [
    "src/components/common/*.tsx",
    "src/features/balance/components/*.tsx",
    "src/features/expenses/components/*.tsx", 
    "src/features/income/components/*.tsx",
    "src/features/history/*.tsx",
    "src/features/history/common/*.tsx",
    "src/hooks/*.ts"
  ],
  "entryPointStrategy": "resolve",
  "out": "docs",
  "exclude": [
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**"
  ],
  "theme": "default",
  "customCss": "./typedoc-custom.css",
  "name": "🏠 家計簿アプリ - APIドキュメント",
  "excludeExternals": true,
  "excludePrivate": false,
  "excludeProtected": false,
  "disableSources": false,
  "includeVersion": true,
  "sort": ["source-order"],
  "readme": "none",
  "navigation": {
    "includeCategories": true,
    "includeGroups": true,
    "includeFolders": true
  },
  "categorizeByGroup": true,
  "kindSortOrder": [
    "Module",
    "Function",
    "Interface",
    "TypeAlias",
    "Variable"
  ],
  "modifierTags": [
    "@component"
  ],
  "sidebarLinks": {
    "GitHub": "https://github.com/user/family-budget-app",
    "🎨 Storybook": "http://localhost:6006"
  },
  "hideGenerator": true,
  "groupOrder": [
    "共通コンポーネント",
    "残高機能", 
    "支出機能",
    "収入機能",
    "履歴機能",
    "カスタムフック"
  ]
}